<style type="text/css">
    #overlay {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: #000;
    filter:alpha(opacity=70);
    -moz-opacity:0.7;
    -khtml-opacity: 0.7;
    opacity: 0.7;
    z-index: 100;
    display: none;
    }
    .content a{
    text-decoration: none;
    }
    .popup{
    width: 100%;
    margin: 0 auto;
    display: none;
    position: fixed;
    z-index: 101;
    }
    .popup1{
    width: 100%;
    margin: 0 auto;
    display: none;
    position: fixed;
    z-index: 101;
    }
    .content{
    min-width: 600px;
    width: 600px;
    min-height: 150px;
    margin-left:150px;
    margin-top:0px;
    /*margin: 0px auto;padding: 10px;*/
    background: #f3f3f3;
    position: relative;
    z-index: 103;

    border-radius: 5px;
    box-shadow: 0 2px 5px #000;
    }
    .content p{
    clear: both;
    color: #555555;
    text-align: justify;
    }
    .content p a{
    color: #d91900;
    font-weight: bold;
    }
    .content .x,.x1{
    float: right;
    height: 35px;
    left: 22px;
    position:relative;
    top: -25px;
    width: 34px;
    }
    .content .x:hover,.x1:hover{
    cursor: pointer;
    }
    .search{
    height: 30px;
    width: 30px;
    }
    .search:hover
    {
    height: 31px;
    width: 31px;
    }
</style>
<title>KMI-INTRANET</title>
</head>
