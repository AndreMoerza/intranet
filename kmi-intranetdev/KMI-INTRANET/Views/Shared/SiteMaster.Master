 <script type="text/javascript">
     $(function () {
         $('#example1').dataTable({
             "bPaginate": true,
             "bLengthChange": true,
             "bFilter": true,
             "bSort": true,
             "bInfo": true,
             "bAutoWidth": false
         });
         $('#example2').dataTable({
             "bPaginate": true,
             "bLengthChange": false,
             "bFilter": true,
             "bSort": true,
             "bInfo": false,
